.page{
    background: #fff;
    position: absolute;
    top:0;
    bottom:0;
    width: 100%;
  }
  .weui-cells__title{
    margin-top:.77em;
  margin-bottom:.3em;
  padding-left:15px;
  padding-right:15px;
  color:#999;
  display: flex;
  margin-top: 100rpx;
  justify-content: center;
  font-size:14px;
   
  }
  /* 虚拟键盘 */
  .keyboard{
    height: auto;
    background: white;
    position: fixed;
    bottom:0;
    width: 100%;
    left:0;
  }
  .keyboard-item{
    padding:10rpx 2rpx 25rpx 2rpx;
    position: relative;
    display: block;
  }
  /* 关闭虚拟键盘 */
  .keyboardClose{
    height: 70rpx;
    background-color: #f7f7f7;
    overflow: hidden;
  }
  .keyboardClose_btn1{
    float: right;
    line-height: 70rpx;
    font-size: 15px;
    padding-right: 30rpx;
    color: red;
  }
  .keyboardClose_btn2{
    float: right;
    line-height: 70rpx;
    font-size: 15px;
    padding-right: 30rpx;
    color: orange;
  }
  /* 虚拟键盘-省缩写 */
   
  /* 虚拟键盘-行 */
  .keyboard-line{
    margin:0 auto;
    text-align: center;
  }
  .iscarnumber .keyboard-line{
    text-align: center;
    margin-left: 2rpx;
  }
  /* 虚拟键盘-单个按钮 */
  .keyboard-btn{
    font-size: 20px;
    color: #333333;
    background: #fff;
    display: inline-block;
    padding:18rpx 0; 
    width: 69rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    border: 1rpx solid #999;
    border-radius: 20rpx;
    margin:10rpx 2rpx;
  }
  .keyboard-btn:active{
    font-size: 25px;
    color: #333333;
    background: #999999;
    display: inline-block;
    padding:18rpx 0; 
    width: 64rpx;
    text-align: center;
    box-shadow: 0 6rpx 5rpx 0 #999999;
    border-radius: 10rpx;
    margin:10rpx 2rpx;
    
  }
   
  /* 虚拟键盘-删除按钮 */
  .keyboard-del{
    font-size: 17px;
    color: #333333;
    background: #A7B0BC;
    display: inline-block;
    padding:4rpx 25rpx;
    box-shadow: 0 2rpx 0 0 #999999;
    border-radius: 10rpx;
    margin:5rpx;
    position: absolute;
    width: 20px;
    height: 30px;
    line-height: 35px;
    bottom:26rpx;
    right: 16rpx;
  }
  .keyboard-del:active{
    font-size: 17px;
    color: #333333;
    background: #fff;
    display: inline-block;
    padding:4rpx 25rpx;
    box-shadow: 0 2rpx 0 0 #999999;
    border-radius: 10rpx;
    margin:5rpx;
    position: absolute;
    width: 20px;
    height: 20px;
    line-height: 40px;
    bottom:26rpx;
    right: 16rpx;
  }
  .keyboard-del-font{
    font-size:25px;
  }
   
  /* 车牌号码 */
  .carNumber-items{
    text-align: center;
   
    margin-top: 80rpx;
  }
  .carNumber-items-box{
    width: 158rpx;
    height: 76rpx;
    border: 2rpx solid #cfcfcf;
    border-radius: 4rpx;
    display: inline-block;
    vertical-align: middle;
    position: relative;
    margin-right: 30rpx;
  }
  .carNumber-items-box-list{
    width: 76rpx;
    height: 76rpx;
    line-height: 70rpx;
    text-align: center;
    display: inline-block;
    font-size: 18px;
    margin:10rpx 0;
    vertical-align: middle;
  }
  .carNumber-items-province{
    border-right: 1rpx solid #cfcfcf;
    margin-top: -10rpx;
  }
  .carNumber-items-box::after{
    content: "";
    width: 6rpx;
    height: 6rpx;
    position: absolute;
    right: -22rpx;
    top: 40rpx;
    border-radius: 50%;
    background-color: #cfcfcf;
  }
  .carNumber-item{
    width: 76rpx;
    height: 76rpx;
    font-size: 18px;
    text-align: center;
    border: 2rpx solid #cfcfcf;
    border-radius: 4rpx;
    line-height: 90rpx;
    display: inline-block;
    margin:0 4rpx;
    vertical-align: middle;
  }
  /* 新能源 */
  .carNumber-item-newpower{
    border: 2rpx dashed #A8BFF3;
    background-color: #F6F9FF;
    color: #A8BFF3;
    font-size: 12px;
    line-height: 35rpx;
  }
  .carNumber-newpower-add{
    font-size: 18px;
  }
   
  /* 确认按钮 */
  .carNumberBtn{
    
    width: 550rpx;
    height: 75rpx;
    line-height: 75rpx;
    font-size: 10pt;
    margin: 100rpx auto;
    background-color: #2063af;
    
    border-radius: 50rpx; 
  }
  .text{
    font-size:10px;
  }
 